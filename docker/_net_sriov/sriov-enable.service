[Unit]
Description=SRIOV enable and configure
After=network.target
Before=docker.service libvirtd.service network-online.target docker-sriov-plugin.service

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/opt/home-scripts/docker/_net_sriov/sriov-enable.sh

[Install]
WantedBy=multi-user.target

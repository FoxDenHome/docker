akvorado.foxden.network {
	route /api/v0/inlet/* {
		uri strip_prefix /api/v0/inlet
		reverse_proxy akvorado-inlet:8080
	}
	reverse_proxy akvorado-console:8080
}

akvorado.foxden.network:8081 {
	reverse_proxy /kafka-ui/* kafka-ui:8080
	route /clickhouse/* {
		uri strip_prefix /clickhouse
		reverse_proxy clickhouse:8123
	}
	reverse_proxy /api/v0/orchestrator/* akvorado-orchestrator:8080
}
